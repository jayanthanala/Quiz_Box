
<%- include("./partials/teheader.ejs") %>
  <link rel="stylesheet" href="/stylesheets/examstudent.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
</head>
<body>
  <nav class="navbar navbar-expand-lg navbar-light bg-light">
  <a class="navbar-brand" href="/">QUIRKY</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarNav">
    <ul class="navbar-nav">
      <li class="nav-item">
        <a class="nav-link" href="/te/exam/upcoming">Home</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/te/exam/upcoming">Exams</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/te/exam/completed">Completed Exams</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/te/exam/staged">Staged Exams</a>
      </li>
    </ul>
  </div>
</nav>

<div class="container">
      <div class="row">
        <div class="col-md-12">
          <h5 class="text-center"> Number of students added : <%=students.length%></h5>
        </div>
      </div>
      <div class="row">
        <div class="col-md-6">
           <h5>Add Students By entering the Roll Number:</h5>
           <form class="" action="/te/exam/<%=id%>/students" method="post">
              <div class="input-field">
                <input type="text" name="student" id="s">
                <label for="s">Student Roll Number</label>
              </div>
            <button type="button" name="button">Submit</button>
           </form>
        </div>
      <div class="col-md-6">
        <h5>Share the given accesscode</h5>
        <div id="copy">
           <%=access%> <i class="far fa-copy"></i>
        </div>
    </div>
  </div>
  <h2>Students</h2>
  <div class="container mx-5 px-3">
    <div class="row">
      Roll Numbers
    </div>
    <% for(var i=0;i<=students.length-1;i++){ %>
      <div class="row">
        <div class="col-md-6">
          <%=i+1%>
        </div>
        <div class="colmd-6">
          <%=students[i]%>
        </div>
      </div>
      <hr>
   <%  }%>
  </div>
</div>














<script src="https://kit.fontawesome.com/30ef029f33.js" crossorigin="anonymous"></script>
<%- include("./partials/tefooter.ejs") %>
