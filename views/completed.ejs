<%- include("partials/teheader.ejs") %>
</head>

<body>


  <%- include("partials/tenav.ejs") %>
  <div class="container" style="padding-top:50px">
    <div class="row row-cols-1 row-cols-md-3">
      <%if(exams[0]){%>
      <% for(var i=0;i<=exams.length-1;i++){%>
      <div class="col mb-4">
        <div class="card h-100">
          <img src="https://nca.legal/wp-content/uploads/2018/08/information-icon.svg" class="m-2 card-img-top" width="100" height="100" alt="...">
          <div class="card-body">
            <h5 class="card-title"><%=exams[i].title%></h5>
            <p class="card-text">Number of questions: <%=exams[i].noquestions%></p>
            <p class="card-text">Total Marks: <%=exams[i].marks%></p>
            <p class="card-text"><a class="btn btn-outline-success" role="button" href="/te/exam/<%=exams[i]._id%>/responses">View Results</a></p>
          </div>
        </div>
      </div>
      <%}%>
    </div>
    <%}else{%>
      <div class="text-center">
        <img class="mx-auto d-block" style="margin: 0 auto" src="https://testbanktop.com/wp-content/uploads/2018/01/search_error.png" width="300" height="300" alt="">
      </div>
      <div class="col-md-12 text-center">
        <h2>No Exams Found</h2>
        <p>You'll find Conducted Exams here</p>
      </div>
      <%  }%>
    </div>
  </div>










  <%- include("./partials/footer.ejs") %>
