
<%- include("./partials/teheader.ejs") %>
</head>
<body>
  <nav class="navbar navbar-expand-lg navbar-light bg-light">
  <a class="navbar-brand" href="/">QUIRKY</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarNav">
    <ul class="navbar-nav">
      <li class="nav-item">
        <a class="nav-link" href="/te/index">Home</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/te/exam/ready">Exams</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/te/exam/completed">Completed Exams</a>
      </li>
      <li class="nav-item active">
        <a class="nav-link" href="/te/exam/staged">Staged Exams<span class="sr-only">(current)</span></a>
      </li>
    </ul>
  </div>
</nav>

<%if(exams[0]){ %>
  <div class="container px-5">
    <div class="container px-5">
      <% for(var i=0;i<=exams.length-1;i++){%>
      <div class="row my-5 py-3 border">
        <div class="col-md-12">
          <h4 class="">Title: <%=exams[i].title%></h4>
        </div>
        <div class="col-md-12">
          <h6 class="">  Number of questions: <%=exams[i].noquestions%></h6>
        </div>
        <div class="col-md-12">
          Total marks: <%=exams[i].marks%>
        </div>
        <div class="col-md-12 d-flex justify-content-end">
            <a href="<%=exams[i]._id%>/edit" class="btn btn-outline-primary">Edit</a>
        </div>
        <div class="col-md-12">
            <form style="border:0px" action="<%=exams[i]._id%>/staged" method="post" enctype="multipart/form-data">
            <input type="file" name="excel">
            <button class="btn btn-outline-success">Submit</button>
          </form>
        </div>
      </div>
    <%}%>
    </div>
  </div>

<%}else{%>
  <h5>NO EXAMS WHICH ARE STAGED !!</h5>
<%}%>



<%- include("./partials/tefooter.ejs") %>
