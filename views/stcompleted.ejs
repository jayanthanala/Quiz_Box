<body>

<%- include("./partials/stnav.ejs") %>

<h3 class="text-center mt-4">Results</h3>

  <%if(exams[0]){%>
    <div class="container px-5">
      <div class="container px-5">
        <% for(var i=0;i<=exams.length-1;i++){%>
          <div class="row my-5 py-3 border">
            <div class="col-md-12">
              <h4 class=""><%=exams[i].title%></h4>
            </div>
            <div class="col-md-12">
              <h6 class="">Number of questions: <%=exams[i].noquestions%></h6>
            </div>
            <div class="col-md-12">
              Total marks: <%=exams[i].marks%>
            </div>
            <div class="col-md-12 d-flex justify-content-around">
                <a href="/st/results/<%=exams[i]._id%>" class="btn btn-outline-primary">View Results</a>
            </div>
          </div>
          <hr>

      <%}%>
      </div>
    </div>
  <%}else{%>
    <div class="container px-5">
      <center>
        <h4 style="padding:30px;border:1px solid black;margin:30px">No Exams Submitted Yet</h4>
        </center>
      </div>
<%  }%>










